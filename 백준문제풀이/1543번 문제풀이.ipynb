   "source": [
    "document = input()\n",
    "word = input()\n",
    "\n",
    "index=0\n",
    "result=0\n",
    "\n",
    "while len(document) - index >= len(word):\n",
    "    if document[index:index+len(word)]==word:\n",
    "        result+=1\n",
    "        index+=len(word)\n",
    "    else:\n",
    "        index+=1\n",
    "        \n",
    "print(result)"
   ]
  }
