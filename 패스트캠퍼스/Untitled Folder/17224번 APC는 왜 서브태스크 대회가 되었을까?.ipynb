   "source": [
    "n,l,k = map(int,input().split())\n",
    "\n",
    "easy,hard =0,0\n",
    "\n",
    "for i in range(n):\n",
    "    sub1,sub2 = map(int,input().split())\n",
    "    if sub2 <=l:\n",
    "        hard+=1\n",
    "    elif sub1 <= l:\n",
    "        easy+=1\n",
    "\n",
    "# hard문제\n",
    "ans = min(hard,k)*140\n",
    "\n",
    "#easy\n",
    "\n",
    "if hard<k:\n",
    "    ans += min(k-hard,easy)*100\n",
    "\n",
    "print(ans)"
   ]
  }
