   "source": [
    "n=int(input())\n",
    "v=list(map(int,input().split()))\n",
    "dp=[1]*n\n",
    "for i in range(n):\n",
    "    for j in range(i):\n",
    "        if v[j]<v[i]:\n",
    "            dp[i]=max(dp[i],dp[j]+1)\n",
    "\n",
    "print(max(dp))"
   ]
